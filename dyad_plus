/*Day1-- dyad plus*/
#include<iostream>
#include<string>
#include<vector>
#include<algorithm>
#include<cmath>
using namespace std;
inline void keep_wondow_open(){char ch; cin>>ch;}

int main(){
    cout<<""<<endl;
    double lval = 0;
    double rval = 0;
    char op;
    double res;
    cin>>lval>>op>>rval; 

    if(op == '+'){
        res = lval + rval;
    }
    else if(op == '-'){
        res = lval - rval;
    }
    cout<<res<<endl;
    return 0;
}
